{
  "extends": "@opentrader/tsconfig/esm.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../../dts/packages/logger",
    "paths": {
      "@opentrader/eslint/*": [
        "../eslint/*"
      ],
      "@opentrader/tsconfig/*": [
        "../tsconfig/*"
      ],
      "@opentrader/types": [
        "../types/src/index.ts"
      ],
      "@opentrader/types/*": [
        "../types/src/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "references": [
    {
      "path": "../eslint"
    },
    {
      "path": "../tsconfig"
    },
    {
      "path": "../types"
    }
  ]
}
